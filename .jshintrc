      "FadeOutInState": true,
      "GameStudioAdvertState": true,
      "Hero": true,
      "LoadingState": true,
      "MapChangeState": true,
      "MapLogic": true,
      "Npc": true,
      "NpcType": true,
      "QuestionBubble": true,
      "QuestionSegment": true,
      "RoamingState": true,
      "RoamingEntity": true,
      "Shield": true,
      "ShoppingBubble": true,
      "Sounds": true,
      "StatBubble": true,
      "StatusBubble": true,
      "TalkManager": true,
      "TalkSegment": true,
      "TextBubble": true,
      "tiled": true,
      "TitleScreenState": true,
      "Weapon": true
   },
   "exported": [ "init", "BattleEntity" ]
}
